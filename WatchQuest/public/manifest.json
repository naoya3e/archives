{
  "manifest_version": 2,
  "name": "WatchQuest",
  "description": "A watcher chrome extension for Amazon Prime Video",
  "version": "0.0.1",
  "permissions": [
    "alarms",
    "notifications",
    "storage"
  ],
  "icons": {
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [ "background.bundle.js" ],
    "persistent": false
  },
  "options_ui": {
    "page": "option.html",
    "chrome_style": true
  }
}
